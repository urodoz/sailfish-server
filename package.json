{
  "name": "sailfish-ci-server",
  "description": "Simple CI Application - Server",
  "version": "0.0.1",
  "author": "Albert Lacarta <urodoz@gmail.com>",
  "dependencies": {
    "async": "^0.9.0",
    "body-parser": "^1.12.2",
    "bunyan": "^1.3.4",
    "colors": "^1.0.3",
    "express": "^4.12.3",
    "express-bunyan-logger": "^1.1.1",
    "fs-extra": "^0.18.0",
    "lodash": "^3.7.0",
    "mongoose": "^4.0.1",
    "node-uuid": "^1.4.3",
    "nodegit": "^0.3.3",
    "randomstring": "^1.0.3",
    "request": "^2.54.0",
    "restler": "^3.2.2",
    "ring": "^2.0.1",
    "slug": "^0.8.0",
    "socket.io": "^1.3.5",
    "ssh-keygen": "^0.2.1",
    "string": "^3.1.1",
    "twig": "^0.8.2",
    "wildcard": "^1.1.0"
  },
  "devDependencies": {
    "chai": "^2.2.0",
    "mocha": "^2.2.1",
    "pre-commit": "^1.0.6",
    "sinon": "^1.14.1"
  },
  "keywords": [
    "continuous integration",
    "testing",
    "ci",
    "cd",
    "ci server"
  ],
  "config": {},
  "main": "run",
  "scripts": {
    "jshint": "jshint ./lib",
    "run": "NODE_PATH=$NODE_PATH:./lib node main.js",
    "test": "NODE_PATH=$NODE_PATH:./lib mocha --recursive test",
    "pm2_start": "NODE_PATH=$NODE_PATH:./lib pm2 start main.js",
    "pm2_stop": "NODE_PATH=$NODE_PATH:./lib pm2 stop main.js",
    "pm2_delete": "NODE_PATH=$NODE_PATH:./lib pm2 delete main.js"
  },
  "pre-commit": [
    "jshint"
  ],
  "engines": {
    "node": ">= 0.10.36"
  }
}
